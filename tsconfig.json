{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./projects/kypo-sandbox-agenda",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "paths": {
      "@muni-kypo-crp/sandbox-agenda": [
        "./src/public_api.ts"
      ],
      "@muni-kypo-crp/sandbox-agenda/internal": [
        "./internal/src/public_api.ts"
      ],
      "@muni-kypo-crp/sandbox-agenda/pool-detail": [
        "./pool-detail/src/public_api.ts"
      ],
      "@muni-kypo-crp/sandbox-agenda/pool-edit": [
        "./pool-edit/src/public_api.ts"
      ],
      "@muni-kypo-crp/sandbox-agenda/pool-overview": [
        "./pool-overview/src/public_api.ts"
      ],
      "@muni-kypo-crp/sandbox-agenda/request-detail": [
        "./request-detail/src/public_api.ts"
      ],
      "@muni-kypo-crp/sandbox-agenda/resolvers": [
        "./resolvers/src/public_api.ts"
      ],
      "@muni-kypo-crp/sandbox-agenda/sandbox-definition-edit": [
        "./sandbox-definition-edit/src/public_api.ts"
      ],
      "@muni-kypo-crp/sandbox-agenda/sandbox-definition-overview": [
        "./sandbox-definition-overview/src/public_api.ts"
      ],
      "@muni-kypo-crp/sandbox-agenda/topology": [
        "./topology/src/public_api.ts"
      ]
    },
    "lib": [
      "es2018",
      "dom"
    ],
    "useDefineForClassFields": false
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
