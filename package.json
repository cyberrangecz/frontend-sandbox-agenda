{
    "name": "sandbox-agenda",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve --ssl --configuration local",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint && npm run prettier-check",
        "e2e": "ng e2e",
        "prettier-check": "prettier --check \"projects/**/*.ts\"",
        "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
        "ci-test": "ng test --browsers ChromeHeadless --watch=false",
        "build-example-app": "ng build sandbox-agenda-example-app --configuration production",
        "build-library": "ng build sandbox-agenda --configuration production",
        "create-package": "cd dist/sandbox-agenda && npm pack",
        "copy-static-files": "cp LICENSE dist/sandbox-agenda && echo \"\n---\nSources: $(git remote get-url origin)\" >> dist/sandbox-agenda/README.md",
        "build-and-pack": "npm install && npm run build-library && npm run copy-static-files && npm run create-package",
        "ci-update-version": "cd projects/sandbox-agenda/ && npm version $TAG_VERSION",
        "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run copy-static-files && npm run create-package",
        "ci-publish-package": "cd dist/sandbox-agenda && npm publish --provenance --access public",
        "prepare": "husky"
    },
    "dependencies": {
        "@angular/animations": "18.2.13",
        "@angular/cdk": "18.2.13",
        "@angular/common": "18.2.13",
        "@angular/compiler": "18.2.13",
        "@angular/core": "18.2.13",
        "@angular/forms": "18.2.13",
        "@angular/material": "18.2.13",
        "@angular/platform-browser": "18.2.13",
        "@angular/platform-browser-dynamic": "18.2.13",
        "@angular/router": "18.2.13",
        "@crczp/sandbox-api": "^1.0.0",
        "@crczp/sandbox-model": "^1.0.0",
        "@crczp/topology-graph": "^1.0.0",
        "@crczp/topology-graph-model": "^1.0.0",
        "@crczp/theme": "^1.0.0",
        "@sentinel/auth": "18.0.3",
        "@sentinel/common": "18.0.4",
        "@sentinel/components": "18.2.7",
        "@sentinel/layout": "18.1.4",
        "angular-oauth2-oidc": "17.0.2",
        "angular-oauth2-oidc-jwks": "17.0.2",
        "d3": "7.9.0",
        "file-saver": "2.0.5",
        "moment-mini": "2.29.4",
        "ngx-indexed-db": "18.0.0",
        "rxjs": "7.8.1",
        "tslib": "2.7.0",
        "typescript-collections": "1.3.3",
        "zone.js": "0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "18.2.14",
        "@angular/cli": "18.2.14",
        "@angular/compiler-cli": "18.2.13",
        "@angular/language-service": "18.2.13",
        "@angular-eslint/builder": "18.4.3",
        "@angular-eslint/eslint-plugin": "18.4.3",
        "@angular-eslint/eslint-plugin-template": "18.4.3",
        "@angular-eslint/schematics": "18.4.3",
        "@angular-eslint/template-parser": "18.4.3",
        "@typescript-eslint/parser": "8.24.0",
        "@typescript-eslint/eslint-plugin": "8.24.0",
        "eslint": "9.20.0",
        "@types/node": "22.6.1",
        "@types/jasmine": "5.1.4",
        "@types/jasminewd2": "2.0.13",
        "codelyzer": "6.0.2",
        "jasmine-core": "5.3.0",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.4.4",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "ng-packagr": "18.2.1",
        "husky": "9.1.7",
        "prettier": "3.5.0",
        "protractor": "~7.0.0",
        "ts-node": "10.9.2",
        "typescript": "5.5.4"
    },
    "optionalDependencies": {
        "@esbuild/android-arm": "0.24.0",
        "fsevents": "2.3.3",
        "@nx/nx-darwin-arm64": "19.8.0"
    }
}
