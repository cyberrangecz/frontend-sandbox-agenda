{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "sandbox-agenda": {
            "projectType": "library",
            "root": "projects/sandbox-agenda",
            "sourceRoot": "projects/sandbox-agenda",
            "prefix": "crczp",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/sandbox-agenda/tsconfig.lib.json",
                        "project": "projects/sandbox-agenda/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/sandbox-agenda/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/sandbox-agenda/test.ts",
                        "tsConfig": "projects/sandbox-agenda/tsconfig.spec.json",
                        "karmaConfig": "projects/sandbox-agenda/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/sandbox-agenda/**/*.ts",
                            "projects/sandbox-agenda/**/*.component.html"
                        ]
                    }
                }
            }
        },
        "sandbox-agenda-example": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/sandbox-agenda-example",
            "sourceRoot": "projects/sandbox-agenda-example/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/sandbox-agenda-example",
                        "index": "projects/sandbox-agenda-example/src/index.html",
                        "main": "projects/sandbox-agenda-example/src/main.ts",
                        "polyfills": "projects/sandbox-agenda-example/src/polyfills.ts",
                        "tsConfig": "projects/sandbox-agenda-example/tsconfig.app.json",
                        "assets": [
                            "projects/sandbox-agenda-example/src/favicon.ico",
                            "projects/sandbox-agenda-example/src/assets"
                        ],
                        "styles": [
                            "projects/sandbox-agenda-example/src/variables.scss",
                            "projects/sandbox-agenda-example/src/theme.scss",
                            "projects/sandbox-agenda-example/src/styles.scss"
                        ],
                        "scripts": [],
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/sandbox-agenda-example/src/environments/environment.ts",
                                    "with": "projects/sandbox-agenda-example/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "local": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/sandbox-agenda-example/src/environments/environment.ts",
                                    "with": "projects/sandbox-agenda-example/src/environments/environment.local.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": ""
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "buildTarget": "sandbox-agenda-example:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "sandbox-agenda-example:build:production"
                        },
                        "local": {
                            "buildTarget": "sandbox-agenda-example:build:local"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "sandbox-agenda-example:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/sandbox-agenda-example/src/test.ts",
                        "polyfills": "projects/sandbox-agenda-example/src/polyfills.ts",
                        "tsConfig": "projects/sandbox-agenda-example/tsconfig.spec.json",
                        "karmaConfig": "projects/sandbox-agenda-example/karma.conf.js",
                        "assets": [
                            "projects/sandbox-agenda-example/src/favicon.ico",
                            "projects/sandbox-agenda-example/src/assets"
                        ],
                        "styles": [
                            "projects/sandbox-agenda-example/src/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/sandbox-agenda-example/**/*.ts",
                            "projects/sandbox-agenda-example/**/*.component.html"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/sandbox-agenda-example/e2e/protractor.conf.js",
                        "devServerTarget": "sandbox-agenda-example:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "sandbox-agenda-example:serve:production"
                        }
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false,
        "cache": {
            "enabled": false
        }
    },
    "schematics": {
        "@angular-eslint/schematics:application": {
            "setParserOptionsProject": true
        },
        "@angular-eslint/schematics:library": {
            "setParserOptionsProject": true
        }
    }
}
