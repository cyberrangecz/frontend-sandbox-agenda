{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "kypo-sandbox-agenda": {
      "projectType": "library",
      "root": "projects/kypo-sandbox-agenda",
      "sourceRoot": "projects/kypo-sandbox-agenda",
      "prefix": "kypo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/kypo-sandbox-agenda/tsconfig.lib.json",
            "project": "projects/kypo-sandbox-agenda/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/kypo-sandbox-agenda/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kypo-sandbox-agenda/test.ts",
            "tsConfig": "projects/kypo-sandbox-agenda/tsconfig.spec.json",
            "karmaConfig": "projects/kypo-sandbox-agenda/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/kypo-sandbox-agenda/**/*.ts",
              "projects/kypo-sandbox-agenda/**/*.component.html"
            ]
          }
        }
      }
    },
    "kypo-sandbox-agenda-example": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/kypo-sandbox-agenda-example",
      "sourceRoot": "projects/kypo-sandbox-agenda-example/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/kypo-sandbox-agenda-example",
            "index": "projects/kypo-sandbox-agenda-example/src/index.html",
            "main": "projects/kypo-sandbox-agenda-example/src/main.ts",
            "polyfills": "projects/kypo-sandbox-agenda-example/src/polyfills.ts",
            "tsConfig": "projects/kypo-sandbox-agenda-example/tsconfig.app.json",
            "assets": [
              "projects/kypo-sandbox-agenda-example/src/favicon.ico",
              "projects/kypo-sandbox-agenda-example/src/assets"
            ],
            "styles": [
              "projects/kypo-sandbox-agenda-example/src/styles.scss",
              "projects/kypo-sandbox-agenda-example/src/theme.scss",
              "projects/kypo-sandbox-agenda-example/src/variables.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/kypo-sandbox-agenda-example/src/environments/environment.ts",
                  "with": "projects/kypo-sandbox-agenda-example/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "local": {
              "fileReplacements": [
                {
                  "replace": "projects/kypo-sandbox-agenda-example/src/environments/environment.ts",
                  "with": "projects/kypo-sandbox-agenda-example/src/environments/environment.local.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kypo-sandbox-agenda-example:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "kypo-sandbox-agenda-example:build:production"
            },
            "local": {
              "browserTarget": "kypo-sandbox-agenda-example:build:local"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kypo-sandbox-agenda-example:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kypo-sandbox-agenda-example/src/test.ts",
            "polyfills": "projects/kypo-sandbox-agenda-example/src/polyfills.ts",
            "tsConfig": "projects/kypo-sandbox-agenda-example/tsconfig.spec.json",
            "karmaConfig": "projects/kypo-sandbox-agenda-example/karma.conf.js",
            "assets": [
              "projects/kypo-sandbox-agenda-example/src/favicon.ico",
              "projects/kypo-sandbox-agenda-example/src/assets"
            ],
            "styles": [
              "projects/kypo-sandbox-agenda-example/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/kypo-sandbox-agenda-example/**/*.ts",
              "projects/kypo-sandbox-agenda-example/**/*.component.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/kypo-sandbox-agenda-example/e2e/protractor.conf.js",
            "devServerTarget": "kypo-sandbox-agenda-example:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "kypo-sandbox-agenda-example:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "cache": {
      "enabled": false
    }
  },
  "schematics": {
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  }
}
